#include <stdio.h>

int main()
{
    int a[3][2]={{2,4},{4,5},{5,3}};
    int b[3][2]={{4,5},{5,8},{8,4}};
    int s1,s2,s3,s4,s5,s6;
    s1=a[0][0]+b[0][0];
    s2=a[0][1]+b[0][1];
    s3=a[1][0]+b[1][0];
    s4=a[1][1]+b[1][1];
    s5=a[2][0]+b[2][0];
    s6=a[2][1]+b[2][1];
    
    printf("%d,%d,%d,%d,%d,%d",s1,s2,s3,s4,s5,s6);
    return 0;
}
